<div class="row">
<h4>Sign up</h4>
<%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
  <%= devise_error_messages! %>
      <div class="row">
      <div class="col s12">
          <div class="input-field inline">
            <%= f.email_field :email, autofocus: true, id: "email", class: "validate" %>
            <label for="email" data-error="wrong" data-success="right">Email</label>
          </div>
      </div>
      <div class="row">
        <div class="col s12">
          <div class="input-field inline">
            <%= f.password_field :password, autocomplete: "off", 
            id: "password", type: "password", class: "validate"%>
            <label for="password" data-error="wrong" data-success="right">password</label>
          </div>
        </div>
      </div>
      <div class="col s12">
          <div class="input-field inline">
            <%= f.password_field :password_confirmation, autocomplete: "off", id: "email", class: "validate" %>
            <label for="password" data-error="wrong" data-success="right">password confirmation</label>
          </div>
      </div>
      <div class="actions">
        <%= f.submit "Sign up",class: "btn waves-effect waves-light", type: "submit", name: "action" %>
      </div>
<% end %> 
</div>

<%= render "devise/shared/links" %>
